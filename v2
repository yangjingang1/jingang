#include <stdio.h>
#include <string.h>

int main()
{
	char strExp[] = "2*2/4*1/1*2*3/2";
	int x = strExp[0] - '0';
	for (int i = 1; i < strlen(strExp); i++)
	{
		if (strExp[i] == '*')
		{
			int a = strExp[i + 1] - '0';
			x = x * a;
			i++;
		}
		else if (strExp[i] == '/')
		{
			int a = strExp[i + 1] - '0';
			x = x / a;
			i++;
		}
	}
    printf("%d\n", x);
	return 0;
}
